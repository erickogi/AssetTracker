<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="12dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Views.Admin.Dashboard.DashBoardAdmin"
    tools:showIn="@layout/activity_dash_board_admin">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/chart"
                android:layout_width="match_parent"
                android:layout_height="300dp" />

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/chartIssues"
                android:layout_width="match_parent"
                android:layout_height="300dp" />

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/chartCust"
                android:layout_width="match_parent"
                android:layout_height="300dp" />

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/chartEng"
                android:layout_width="match_parent"
                android:layout_height="300dp" />

        </LinearLayout>

    </ScrollView>

    <com.anychart.anychart.AnyChartView
        android:visibility="gone"
        android:id="@+id/any_chart_view"

        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_margin="8dp"
        android:layout_weight="1" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/dark_greyish" />

    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="4dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <com.anychart.anychart.AnyChartView
            android:id="@+id/any_chart_issues"
            android:layout_width="0dp"
            android:layout_height="200dp"
            android:layout_margin="6dp"
            android:layout_weight="1" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/dark_greyish" />

        <com.anychart.anychart.AnyChartView
            android:id="@+id/any_chart_users"
            android:layout_width="0dp"
            android:layout_height="200dp"
            android:layout_margin="6dp"
            android:layout_weight="1" />
    </LinearLayout>

